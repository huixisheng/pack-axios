{
  "presets": [
    "@babel/typescript",
    ["@babel/preset-env", {
      "targets": {
        "browsers": [
          "Android >= 4.0",
          "ios >= 6",
          "Edge >= 10",
          "Firefox >= 20",
          "Safari >= 6",
          "Chrome >= 12",
          "ChromeAndroid >= 4.0",
          "QQAndroid >= 1.2"
        ]
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
