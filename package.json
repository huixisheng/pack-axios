{
  "name": "pack-axios",
  "version": "1.0.0",
  "description": "axios二次封装",
  "main": "dist/pack-axios.cjs.js",
  "module": "dist/pack-axios.esm.js",
  "browser": "dist/pack-axios.iife.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "eslint:diff": "npx eslint $(git diff-index HEAD --name-only --diff-filter ACMR |grep '\\.\\(ts\\|tsx\\|jsx\\|es6\\|js\\|vue\\)$')",
    "eslint": "npx eslint src test --ext .tsx,.ts,.js,.tsx",
    "mock": "json-server mock/db.json --routes mock/routes.json --quiet",
    "build:types": "tsc --emitDeclarationOnly",
    "build:babel": "babel src --out-dir dist --extensions '.ts,.tsx'",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:babel",
    "build:ts": "npx tsc --build tsconfig.json"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "axios",
    "ajax",
    "axios-pack"
  ],
  "author": "huixisehng@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "axios": "^0.19.0",
    "es6-promise": "^4.2.8",
    "object-assign": "^4.1.1",
    "qs": "^6.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-async-to-generator": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@rollup/plugin-json": "^4.0.1",
    "@types/jest": "^24.0.25",
    "@types/qs": "^6.9.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "autoprefixer": "^9.7.3",
    "eslint": "^6.8.0",
    "eslint-config-style-guide": "0.0.8",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.19.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^24.9.0",
    "json-server": "^0.15.1",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-nodent": "^0.2.2",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  }
}
